    "cooking-saladcss": "^0.4.0",
    "cooking-vue": "^0.5.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.8.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
